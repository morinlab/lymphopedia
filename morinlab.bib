@article{yugamiHnRNPUEnhancesExpression2007,
  title = {{{hnRNP-U}} Enhances the Expression of Specific Genes by Stabilizing {{mRNA}}},
  author = {Yugami, Masato and Kabe, Yasuaki and Yamaguchi, Yuki and Wada, Tadashi and Handa, Hiroshi},
  date = {2007-01-09},
  journaltitle = {FEBS Letters},
  shortjournal = {FEBS Letters},
  volume = {581},
