  keywords = {Americas,Animals,Biological Evolution,Cell Nucleus,Dog Diseases,Dogs,Domestication,Genome Mitochondrial,Human Migration,Humans,Neoplasms,Phylogeny,Sexually Transmitted Diseases,Siberia,Wolves}
}

