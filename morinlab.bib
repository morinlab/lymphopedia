  author = {Lefranc, Marie-Paule and Giudicelli, Véronique and Duroux, Patrice and Jabado-Michaloud, Joumana and Folch, Géraldine and Aouinti, Safa and Carillon, Emilie and Duvergey, Hugo and Houles, Amélie and Paysan-Lafosse, Typhaine and Hadi-Saljoqi, Saida and Sasorith, Souphatta and Lefranc, Gérard and Kossida, Sofia},
