@article{gintherRaceEthnicityNIH2011,
  title = {Race, Ethnicity, and {{NIH}} Research Awards},
  author = {Ginther, Donna K. and Schaffer, Walter T. and Schnell, Joshua and Masimore, Beth and Liu, Faye and Haak, Laurel L. and Kington, Raynard},
