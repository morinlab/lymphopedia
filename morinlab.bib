@article{daviesComparisonMHGDZsig2023,
  title = {Comparison of {{MHG}} and {{DZsig}} Reveals Shared Biology and a Core Overlap Group with Inferior Prognosis in {{DLBCL}}},
  author = {Davies, John R. and Hilton, Laura K. and Jiang, Aixiang and Barrans, Sharon and Burton, Catherine and Johnson, Peter W. M. and Davies, Andrew J. and Du, Ming-Qing and Tooze, Reuben and Cucco, Francesco and Care, Matthew A. and Morin, Ryan D. and Steidl, Christian and Sha, Chulin and Westhead, David R. and Scott, David W.},
  date = {2023-10-24},
