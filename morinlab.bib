@article{okosunRecurrentMTORC1activatingRRAGC2016a,
  title = {Recurrent {{mTORC1-activating RRAGC}} Mutations in Follicular Lymphoma},
  author = {Okosun, Jessica and Wolfson, Rachel L. and Wang, Jun and Araf, Shamzah and Wilkins, Lucy and Castellano, Brian M. and Escudero-Ibarz, Leire and Al Seraihi, Ahad Fahad and Richter, Julia and Bernhart, Stephan H. and Efeyan, Alejo and Iqbal, Sameena and Matthews, Janet and Clear, Andrew and Guerra-Assunção, José Afonso and Bödör, Csaba and Quentmeier, Hilmar and Mansbridge, Christopher and Johnson, Peter and Davies, Andrew and Strefford, Jonathan C. and Packham, Graham and Barrans, Sharon and Jack, Andrew and Du, Ming-Qing and Calaminici, Maria and Lister, T. Andrew and Auer, Rebecca and Montoto, Silvia and Gribben, John G. and Siebert, Reiner and Chelala, Claude and Zoncu, Roberto and Sabatini, David M. and Fitzgibbon, Jude},
  date = {2016-02},
