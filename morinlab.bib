@article{daiCharacterizationMouseDazap12001,
  title = {Characterization of the Mouse {{Dazap1}} Gene Encoding an {{RNA-binding}} Protein That Interacts with Infertility Factors {{DAZ}} and {{DAZL}}},
  author = {Dai, Tiane and Vera, Yanira and Salido, Eduardo C and Yen, Pauline H},
  date = {2001-09-26},
  journaltitle = {BMC Genomics},
