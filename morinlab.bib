@article{parkHeterogeneousNuclearRibonucleoprotein2017,
  title = {Heterogeneous {{Nuclear Ribonucleoprotein A2B1 Exerts}} a {{Regulatory Role}} in {{Lipopolysaccharide-stimulated 38B9 B Cell Activation}}},
  author = {Park, Jisang and Choe, Chung-Hyeon and Kim, Ju and Yang, Jing Shian and Kim, Jin Hyun and Jang, Hyonseok and Jang, Yong-Suk},
