  pmcid = {PMC9259584},
  keywords = {Adult,Burkitt Lymphoma,Cell Cycle Proteins,Child,DNA Helicases,Genes cdc,Humans,Mutation,Mutation Rate,Neoplasm Recurrence Local,Nuclear Proteins,Transcription Factors}
}


