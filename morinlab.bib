@article{chongComprehensiveCharacterizationProgrammed2016,
  title = {Comprehensive Characterization of Programmed Death Ligand Structural Rearrangements in {{B-cell}} Non-{{Hodgkin}} Lymphomas},
  author = {Chong, Lauren C. and Twa, David D. W. and Mottok, Anja and Ben-Neriah, Susana and Woolcock, Bruce W. and Zhao, Yongjun and Savage, Kerry J. and Marra, Marco A. and Scott, David W. and Gascoyne, Randy D. and Morin, Ryan D. and Mungall, Andrew J. and Steidl, Christian},
