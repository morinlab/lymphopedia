@article{ramanathanMRNACappingBiological2016,
  title = {{{mRNA}} Capping: Biological Functions and Applications},
  shorttitle = {{{mRNA}} Capping},
  author = {Ramanathan, Anand and Robb, G. Brett and Chan, Siu-Hong},
  date = {2016-09-19},
